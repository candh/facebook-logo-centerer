{
  "manifest_version": 2,

  "name": "Facebook Logo Centerer",
  "description": "This extension centers the facebook logo. Perfect for perfectionists.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["http://www.facebook.com/*","https://www.facebook.com/*"],
      "js": ["popup.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}